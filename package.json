{
  "name": "bulls-and-cows",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-intl": "^2.7.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-transition-group": "^2.4.0",
    "redux": "^4.0.0",
    "redux-act": "^1.7.4",
    "redux-saga": "^0.16.0",
    "styled-components": "^3.4.5"
  },
  "scripts": {
    "start": "NODE_PATH=src react-app-rewired start",
    "build": "NODE_PATH=src react-app-rewired build",
    "test": "NODE_PATH=src react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "extract-messages": "NODE_ENV=development ./node_modules/.bin/extract-messages -l=en,ru -o src/i18n/ --flat 'src/**/*.js'"
  },
  "devDependencies": {
    "babel-plugin-react-intl-auto": "^1.1.1",
    "extract-react-intl-messages": "^0.11.1",
    "node-sass-chokidar": "^1.3.4",
    "react-app-rewired": "^1.6.2",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6"
  }
}
